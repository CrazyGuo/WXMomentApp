        <image class="lives-img" src='{{item.url}}' mode='aspectFill' />
        <image class="lives-right-img" src='../../images/play.png' mode='aspectFill' />
      
        </view>
        <text class="lives-name">{{item.title}}</text>

    </view>
  </view>

</view>

