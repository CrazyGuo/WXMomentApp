      </view>
        
        
  </view>


  <!-- 直播内容 -->
  <view class="layout_horizontal">
    
    <view bindtap='detailpage'  data-userid='0' data-usertype='lives' class="lives-content"  wx:for="{{list}}" wx:for-item="item" for:key="key">
